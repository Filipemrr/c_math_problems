#include <stdio.h> 
#include <math.h>
// PROBLEM: 
// PRINT 1 UNTIL N, ( such that N ITS A USER IMPUT)

int ascending(int n) 
{ 
    if (n < 1) 
    {
         return 1; 
    }
        printf ("%d\n", ascending(n-1)); 
        return n; 
}

int main () 
{
    int n; 

    scanf ("%d", &n);  

    ascending(n+1);  
    
return 0; 
}
