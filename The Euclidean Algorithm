#include <stdio.h>
#include <math.h>

int mdc(int a, int b) { //Caculo do MDC 
        if (a%b ==0)
        {
            return b;
        }
        else 
        {
        return mdc(b,a%b); 
        }
}
int loopscan(int x) // a funcao que vai fazer o loop do scan e chamar a outra que calcula o MDC
{
    
    if (x==0) 
    {
        return 0; 
    }
        int n1,n2; 
        scanf("%d%d", &n1, &n2); 
        printf ("MDC(%d,%d) = %d\n", n1,n2, mdc(n1,n2));
        loopscan(x-1); 
        return 0;
}   

    int main () {
        int numero_de_operacoes; 

        scanf ("%d", &numero_de_operacoes); 
        loopscan(numero_de_operacoes);
    }
    
