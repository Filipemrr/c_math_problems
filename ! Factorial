
#include <stdio.h> 
#include <math.h> 


unsigned long fatorial(int n) {
if (n == 0) //passo base 
    {
        return 1;
    }
    else  // Passo Recursivo
    {
        return n * fatorial(n-1); 
    }
}

int main ()
{
    unsigned long  number, res; 
    scanf("%ld", &number); 
    res = fatorial(number); 
    printf ("fatorial de %ld = %ld\n", number,res);
    return 0;
}
